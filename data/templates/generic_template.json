{
  "name": "generic_template",
  "description": "General extraction patterns for RFPs and funding opportunities",
  "created_at": "2024-01-01T00:00:00",
  "extraction_configs": {
    "title": {
      "field_name": "title",
      "patterns": [
        "Title:\\s*([^\\n]+)",
        "Project Title:\\s*([^\\n]+)",
        "Request for Proposals?:?\\s*([^\\n]+)"
      ],
      "extraction_type": "single",
      "post_process": null,
      "required": true,
      "default_value": ""
    },
    "abstract": {
      "field_name": "abstract",
      "patterns": [
        "Abstract:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Summary:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Overview:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Description:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)"
      ],
      "extraction_type": "single",
      "post_process": null,
      "required": true,
      "default_value": ""
    },
    "required_skills": {
      "field_name": "required_skills",
      "patterns": [
        "Required.*?Skills?.*?:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Expertise.*?:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Qualifications.*?:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)",
        "Technical.*?Requirements.*?:\\s*(.*?)(?=\\n\\n|\\n[A-Z][a-z]+:)"
      ],
      "extraction_type": "multiple",
      "post_process": null,
      "required": true,
      "default_value": ""
    },
    "funding_amount": {
      "field_name": "funding_amount",
      "patterns": [
        "Budget:\\s*\\$?\\s*([\\d,]+(?:\\.\\d{2})?)",
        "Funding.*?Amount:\\s*\\$?\\s*([\\d,]+(?:\\.\\d{2})?)",
        "Award.*?Amount:\\s*\\$?\\s*([\\d,]+(?:\\.\\d{2})?)",
        "\\$\\s*([\\d,]+(?:\\.\\d{2})?)"
      ],
      "extraction_type": "single",
      "post_process": null,
      "required": false,
      "default_value": ""
    },
    "deadline": {
      "field_name": "deadline",
      "patterns": [
        "Deadline:\\s*([^\\n]+)",
        "Application.*?Deadline:\\s*([^\\n]+)",
        "Submission.*?Date:\\s*([^\\n]+)",
        "Due.*?Date:\\s*([^\\n]+)"
      ],
      "extraction_type": "single",
      "post_process": null,
      "required": false,
      "default_value": ""
    },
    "program": {
      "field_name": "program",
      "patterns": [
        "Program:\\s*([^\\n]+)",
        "Funding.*?Program:\\s*([^\\n]+)",
        "RFP\\s+([A-Z0-9-]+)"
      ],
      "extraction_type": "single",
      "post_process": null,
      "required": false,
      "default_value": ""
    }
  }
}